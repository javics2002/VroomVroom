material Material/roja
{
	receive_shadows on
	technique
	{
		pass
		{
			diffuse 1.0 0.0 0.0 1.0
			rtshader_system {
                lighting_stage metal_roughness
                texturing_stage late_add_blend
            }
		}
	}
}

